!Objetivo del caso de prueba: Repeat Do While de forma exitosa
!Disenno del caso de prueba: 

let
  var myVar : Integer
in
  myVar := 0;
  repeat do 
    let
      var myVar2 : Integer
    in
      myVar2 := myVar
    end;
    myVar := myVar + 1
  while true
  end
  
end

